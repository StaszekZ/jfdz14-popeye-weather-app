{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "import/no-named-as-default": 0,
    "react/state-in-constructor": 0
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true
  }
}
